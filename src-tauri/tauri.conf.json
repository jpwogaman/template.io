{
  "$schema": "./gen/schemas/acl-manifests.json",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "category": "Productivity",
    "copyright": "Copyright Â©2022 JP Wogaman II",
    "icon": [
      "icons/icon.ico",
      "icons/icon.png"
    ]
  },
  "productName": "template-io",
  "mainBinaryName": "template-io",
  "version": "0.1.0",
  "identifier": "template-io",
  "plugins": {},
  "app": {
    "windows": [
      {
        "title": "Template.io",
        "fullscreen": false,
        "resizable": true,
        "alwaysOnTop": false,
        "minHeight": 600,
        "minWidth": 1000
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "migrated",
        "desktop-capability",
        "fs",
        "log"
      ]
    }
  }
}