{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "category": "Productivity",
    "copyright": "Copyright Â©2022 JP Wogaman II",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "template-io",
  "mainBinaryName": "template-io",
  "version": "0.1.0",
  "identifier": "template-io",
  "plugins": {},
  "app": {
    "windows": [
      {
        "title": "Template.io",
        "fullscreen": false,
        "resizable": true,
        "alwaysOnTop": false,
        "center": true,
        "height": 800,
        "minHeight": 600,
        "width": 1200,
        "minWidth": 1000
      }
    ],
    "security": {
      "csp": null
